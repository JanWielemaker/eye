#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/turnaround-f2b.n3 http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/socrates-query.n3 > socrates-turnaround-f2b-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/turnaround-f2b.n3 http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-turnaround-f2b/socrates-query.n3 > socrates-turnaround-f2b-proof.n3
