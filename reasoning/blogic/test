#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/slide32.n3 --pass > slide32-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/socrates.n3 --pass > socrates-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/abc.n3 --pass > abc-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope --blogic http://josd.github.io/eye/reasoning/blogic/abcd.n3 --pass > abcd-answer.n3
