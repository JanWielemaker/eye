PREFIX math: <http://www.w3.org/2000/10/swap/math#>
PREFIX log: <http://www.w3.org/2000/10/swap/log#>
PREFIX : <http://josd.github.io/eye/reasoning#>

(_:X _:Y) log:negativeSurface {
    () log:negativeSurface {
        _:X :fibonacci _:Y.
    }.
    (_:X 0 1) :fib _:Y.
}.

(_:A _:B) log:negativeSurface {
    () log:negativeSurface {
        (0 _:A _:B) :fib _:A.
    }.
}.
(_:A _:B) log:negativeSurface {
    () log:negativeSurface {
        (1 _:A _:B) :fib _:B.
    }.
}.
(_:A _:B _:X _:Y) log:negativeSurface {
    () log:negativeSurface {
        (_:X _:A _:B) :fib _:Y.
    }.
    _:X math:greaterThan 1.
    (_:X 1) math:difference _:C.
    (_:A _:B) math:sum _:D.
    (_:C _:B _:D) :fib _:Y.
}.

# query
(_:X0 _:X1 _:X2 _:X6 _:X91 _:X283 _:X3674) log:querySurface {
    1 :fibonacci _:X1.
    2 :fibonacci _:X2.
    3 :fibonacci _:X3.
    6 :fibonacci _:X6.
    91 :fibonacci _:X91.
    283 :fibonacci _:X283.
    3674 :fibonacci _:X3674.
}.
