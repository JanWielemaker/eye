#!/bin/bash
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/cs . --nope https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/data-001.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/rules-001.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/query-001.n3 > answer-001.n3
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/cs . https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/data-001.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/rules-001.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/cs/query-001.n3 > proof-001.n3
