#!/bin/bash
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/cs . --nope https://github.com/josd/eye/blob/master/reasoning/cs/data-001.n3 https://github.com/josd/eye/blob/master/reasoning/cs/rules-001.n3 --query https://github.com/josd/eye/blob/master/reasoning/cs/query-001.n3 > answer-001.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/cs . https://github.com/josd/eye/blob/master/reasoning/cs/data-001.n3 https://github.com/josd/eye/blob/master/reasoning/cs/rules-001.n3 --query https://github.com/josd/eye/blob/master/reasoning/cs/query-001.n3 > proof-001.n3
