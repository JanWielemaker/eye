#!/bin/bash
eye "$@" --nope --pl https://github.com/josd/eye/blob/master/reasoning/queens/queens_program.pl --query https://github.com/josd/eye/blob/master/reasoning/queens/queens_query.n3 > queens_answer.n3
eye "$@" --pl https://github.com/josd/eye/blob/master/reasoning/queens/queens_program.pl --query https://github.com/josd/eye/blob/master/reasoning/queens/queens_query.n3 > queens_proof.n3
