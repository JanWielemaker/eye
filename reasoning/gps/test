#!/bin/bash
eye "$@" --n3 https://github.com/josd/eye/blob/master/reasoning/gps/gps-plugin.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/gps/gps-example1.n3 --query https://github.com/josd/eye/blob/master/reasoning/gps/gps-query1.n3 > gps-proof1.n3
eye "$@" --n3 https://github.com/josd/eye/blob/master/reasoning/gps/gps-plugin.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/gps/gps-example2.n3 --query https://github.com/josd/eye/blob/master/reasoning/gps/gps-query2.n3 > gps-proof2.n3
