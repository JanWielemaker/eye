#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/turnaround-b2f.n3 http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/socrates-query.n3 > socrates-turnaround-b2f-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/turnaround-b2f.n3 http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-turnaround-b2f/socrates-query.n3 > socrates-turnaround-b2f-proof.n3
