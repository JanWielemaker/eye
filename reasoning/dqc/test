#!/bin/bash
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/dqc . --no-distinct-output --nope https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-q.n3 > sdcoding-a.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/dqc . --no-distinct-output https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-q.n3 > sdcoding-a-proof.n3

eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/dqc . --no-distinct-input --nope https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-a.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-query.n3 > sdcoding-answer.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/dqc . --no-distinct-input https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-a.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-query.n3 > sdcoding-proof.n3
