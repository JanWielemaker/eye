#!/bin/bash
eye "$@" --no-distinct-output --nope --n3 https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-q.n3 > sdcoding-a.n3
eye "$@" --no-distinct-input --nope --n3 https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-a.n3 --query https://github.com/josd/eye/blob/master/reasoning/dqc/sdcoding-query.n3 > sdcoding-answer.n3
