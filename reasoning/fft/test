#!/bin/bash
eye "$@" --nope --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/fft/fft_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/fft/fft_query.n3 > fft_answer.n3
eye "$@" --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/fft/fft_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/fft/fft_query.n3 > fft_proof.n3
