#!/bin/bash
eye --nope --turtle https://github.com/josd/eye/blob/master/reasoning/ccd/train_data.ttl --n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/ccd/train_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/train_query.n3 > train_model.ttl
eye --nope --turtle https://github.com/josd/eye/blob/master/reasoning/ccd/test_data.ttl --turtle https://github.com/josd/eye/blob/master/reasoning/ccd/train_model.ttl --n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/ccd/ccd_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/test_query.n3 > test_answer.ttl
