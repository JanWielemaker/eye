#!/bin/bash
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/ccd . --nope https://github.com/josd/eye/blob/master/reasoning/ccd/train_data.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/train_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/train_query.n3 > train_model.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/ccd . https://github.com/josd/eye/blob/master/reasoning/ccd/train_data.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/train_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/train_query.n3 > train_model_proof.n3

eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/ccd . --nope https://github.com/josd/eye/blob/master/reasoning/ccd/test_data.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/train_model.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/ccd_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/test_query.n3 > test_answer.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/ccd . https://github.com/josd/eye/blob/master/reasoning/ccd/test_data.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/train_model.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/background_rules.n3 https://github.com/josd/eye/blob/master/reasoning/ccd/ccd_rules.n3 --query https://github.com/josd/eye/blob/master/reasoning/ccd/test_query.n3 > test_proof.n3
