#!/bin/bash
eye "$@" --nope https://github.com/josd/eye/blob/master/reasoning/polygon/polygon.n3 --query https://github.com/josd/eye/blob/master/reasoning/polygon/polygon_query.n3 > polygon_answer.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/polygon/polygon.n3 --query https://github.com/josd/eye/blob/master/reasoning/polygon/polygon_query.n3 > polygon_proof.n3
