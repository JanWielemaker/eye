#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates/socrates-solid.n3 --query http://josd.github.io/eye/reasoning/socrates/socrates-query.n3 > socrates-proof.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates/socrates-solid.n3 --entail http://josd.github.io/eye/reasoning/socrates/socrates-claim.n3 > socrates-check.n3
