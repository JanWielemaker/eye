#!/bin/bash
eye "$@" --n3 https://github.com/josd/eye/blob/master/reasoning/bmi/bmi_instances.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/bmi/bmi_rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/bmi/weightStatus_rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/bmi/age_rules_backward.n3 --query https://github.com/josd/eye/blob/master/reasoning/bmi/bmi_query.n3 > bmi_proof.n3
