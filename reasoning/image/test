#!/bin/bash
eye "$@" https://raw.githubusercontent.com/josd/eye/master/reasoning/image/year.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/image/easter.n3 --image ype.pvm
swipl -x ype.pvm -- "$@" --query https://raw.githubusercontent.com/josd/eye/master/reasoning/image/query.n3 > easter-proof.n3
