#!/bin/bash
eye "$@" --nope --pl https://github.com/josd/eye/blob/master/reasoning/lee/lee_program.pl --query https://github.com/josd/eye/blob/master/reasoning/lee/lee_query.n3 > lee_answer.n3
eye "$@" --pl https://github.com/josd/eye/blob/master/reasoning/lee/lee_program.pl --query https://github.com/josd/eye/blob/master/reasoning/lee/lee_query.n3 > lee_proof.n3
