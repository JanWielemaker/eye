#!/bin/bash
eye "$@" --nope --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/lee/lee_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/lee/lee_query.n3 > lee_answer.n3
eye "$@" --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/lee/lee_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/lee/lee_query.n3 > lee_proof.n3
