#!/bin/bash
eye "$@" --nope https://github.com/josd/eye/blob/master/reasoning/n3c/components.n3 --turtle https://github.com/josd/eye/blob/master/reasoning/n3c/tasks.n3 --query https://github.com/josd/eye/blob/master/reasoning/n3c/notIn_query.n3 > notIn_answer.n3
eye "$@" --nope https://github.com/josd/eye/blob/master/reasoning/n3c/components.n3 --turtle https://github.com/josd/eye/blob/master/reasoning/n3c/tasks.n3 --query https://github.com/josd/eye/blob/master/reasoning/n3c/forAllIn_query.n3 > forAllIn_answer.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/n3c/components.n3 --turtle https://github.com/josd/eye/blob/master/reasoning/n3c/tasks.n3 --query https://github.com/josd/eye/blob/master/reasoning/n3c/notIn_query.n3 > notIn_proof.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/n3c/components.n3 --turtle https://github.com/josd/eye/blob/master/reasoning/n3c/tasks.n3 --query https://github.com/josd/eye/blob/master/reasoning/n3c/forAllIn_query.n3 > forAllIn_proof.n3
