#!/bin/bash
eye "$@" --n3 https://github.com/josd/eye/blob/master/reasoning/fcm/fcm-plugin.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fcm/fl-rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fcm/example001P.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fcm/example002P.n3 --query https://github.com/josd/eye/blob/master/reasoning/fcm/example003Q.n3 > fcm_proof.n3
