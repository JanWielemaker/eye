#!/bin/bash
eye "$@" --n3 https://github.com/josd/eye/blob/master/reasoning/fgcm/fgcm-plugin.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fcm/fl-rules.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fgcm/patient.n3 --n3 https://github.com/josd/eye/blob/master/reasoning/fgcm/fgcm-model.n3 --query https://github.com/josd/eye/blob/master/reasoning/fgcm/fgcm-query.n3 > fgcm_proof.n3
