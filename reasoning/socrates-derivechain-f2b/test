#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/derivechain.n3 http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/socrates-query.n3 > socrates-derivechain-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/derivechain.n3 http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-derivechain-f2b/socrates-query.n3 > socrates-derivechain-proof.n3
