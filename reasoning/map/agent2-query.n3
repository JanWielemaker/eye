PREFIX gps: <https://github.com/josd/eye/blob/master/reasoning/gps/gps-schema#>
PREFIX : <https://github.com/josd/eye/blob/master/reasoning#>

{
    ?SCOPE gps:findpath ({?SUBJECT :location :Oostende} ?PATH ?DURATION ?COST ?BELIEF ?COMFORT (3600.0 5.0 0.2 0.4 1)).
