#!/bin/bash
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/map . --nope https://github.com/josd/eye/blob/master/reasoning/map/gps-plugin.n3 https://github.com/josd/eye/blob/master/reasoning/map/agent1-map.n3 --query https://github.com/josd/eye/blob/master/reasoning/map/agent1-query.n3 > agent1-answer.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/map . https://github.com/josd/eye/blob/master/reasoning/map/gps-plugin.n3 https://github.com/josd/eye/blob/master/reasoning/map/agent1-map.n3 --query https://github.com/josd/eye/blob/master/reasoning/map/agent1-query.n3 > agent1-proof.n3

eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/map . --nope https://github.com/josd/eye/blob/master/reasoning/map/gps-plugin.n3 https://github.com/josd/eye/blob/master/reasoning/map/agent2-map.n3 --proof https://github.com/josd/eye/blob/master/reasoning/map/agent1-proof.n3 --query https://github.com/josd/eye/blob/master/reasoning/map/agent2-query.n3 > agent2-answer.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/map . https://github.com/josd/eye/blob/master/reasoning/map/gps-plugin.n3 https://github.com/josd/eye/blob/master/reasoning/map/agent2-map.n3 --proof https://github.com/josd/eye/blob/master/reasoning/map/agent1-proof.n3 --query https://github.com/josd/eye/blob/master/reasoning/map/agent2-query.n3 > agent2-proof.n3
