#!/bin/bash
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/map . --nope https://raw.githubusercontent.com/josd/eye/master/reasoning/map/gps-plugin.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-map.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-query.n3 > agent1-answer.n3
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/map . https://raw.githubusercontent.com/josd/eye/master/reasoning/map/gps-plugin.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-map.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-query.n3 > agent1-proof.n3

eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/map . --nope https://raw.githubusercontent.com/josd/eye/master/reasoning/map/gps-plugin.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent2-map.n3 --proof https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-proof.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent2-query.n3 > agent2-answer.n3
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/map . https://raw.githubusercontent.com/josd/eye/master/reasoning/map/gps-plugin.n3 https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent2-map.n3 --proof https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent1-proof.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/map/agent2-query.n3 > agent2-proof.n3
