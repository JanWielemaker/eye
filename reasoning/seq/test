#!/bin/bash
eye "$@" --nope --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/seq . https://raw.githubusercontent.com/josd/eye/master/reasoning/seq/seq_components.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/seq/seq_query.n3 > seq_answer.n3
eye "$@" --wcache https://raw.githubusercontent.com/josd/eye/master/reasoning/seq . https://raw.githubusercontent.com/josd/eye/master/reasoning/seq/seq_components.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/seq/seq_query.n3 > seq_proof.n3
