#!/bin/bash
eye "$@" --nope --wcache https://github.com/josd/eye/blob/master/reasoning/seq . https://github.com/josd/eye/blob/master/reasoning/seq/seq_components.n3 --query https://github.com/josd/eye/blob/master/reasoning/seq/seq_query.n3 > seq_answer.n3
eye "$@" --wcache https://github.com/josd/eye/blob/master/reasoning/seq . https://github.com/josd/eye/blob/master/reasoning/seq/seq_components.n3 --query https://github.com/josd/eye/blob/master/reasoning/seq/seq_query.n3 > seq_proof.n3
