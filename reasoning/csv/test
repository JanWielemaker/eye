#!/bin/bash
eye "$@" --nope https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-data.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-query.n3 --strings > select-data.csv
eye "$@" https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-data.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-query.n3 > select-proof.n3
eye "$@" https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-data-extra.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/csv/select-query.n3 > select-proof-extra.n3
