#!/bin/bash
eye "$@" --nope --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_interpreter.pl --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_query.n3 > utm_answer.n3
eye "$@" --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_interpreter.pl --pl https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_program.pl --query https://raw.githubusercontent.com/josd/eye/master/reasoning/utm/utm_query.n3 > utm_proof.n3
