#!/bin/bash
eye "$@" --nope https://github.com/josd/eye/blob/master/reasoning/bi/biP.n3 --query https://github.com/josd/eye/blob/master/reasoning/bi/biQ.n3 > biA.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/bi/biP.n3 --query https://github.com/josd/eye/blob/master/reasoning/bi/biQ.n3 > biE.n3
eye "$@" --nope https://github.com/josd/eye/blob/master/reasoning/bi/math_flowc.n3 --pass > math_flowc_answer.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/bi/math_flowc.n3 --pass > math_flowc_proof.n3

