#!/bin/bash
eye "$@" https://github.com/josd/eye/blob/master/reasoning/witch/witch.n3 --query https://github.com/josd/eye/blob/master/reasoning/witch/witch-goal.n3 > witch-proof.n3
eye "$@" https://github.com/josd/eye/blob/master/reasoning/witch/floatingwoman.n3 --proof https://github.com/josd/eye/blob/master/reasoning/witch/witch-proof.n3 --query https://github.com/josd/eye/blob/master/reasoning/witch/floatingwoman-goal.n3 > floatingwoman-proof.n3
