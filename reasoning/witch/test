#!/bin/bash
eye "$@" https://raw.githubusercontent.com/josd/eye/master/reasoning/witch/witch.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/witch/witch-goal.n3 > witch-proof.n3
eye "$@" https://raw.githubusercontent.com/josd/eye/master/reasoning/witch/floatingwoman.n3 --proof https://raw.githubusercontent.com/josd/eye/master/reasoning/witch/witch-proof.n3 --query https://raw.githubusercontent.com/josd/eye/master/reasoning/witch/floatingwoman-goal.n3 > floatingwoman-proof.n3
