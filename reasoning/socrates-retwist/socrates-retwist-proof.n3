@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix : <http://example.org/socrates#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix var: <http://josd.github.io/var#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.

[] a r:Proof, r:Conjunction;
  r:component <#lemma1>;
  r:component <#lemma2>;
  r:gives {
    :Socrates a :Human.
    :Socrates a :Mortal.
  }.

<#lemma1> a r:Inference;
  r:gives {
    :Socrates a :Human.
  };
  r:evidence (
    <#lemma3>
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/socrates#Human"]];
  r:rule <#lemma4>.

<#lemma2> a r:Inference;
  r:gives {
    :Socrates a :Mortal.
  };
  r:evidence (
    [ a r:Fact; r:gives {:Socrates a :Mortal}]
  );
  r:binding [ r:variable [ n3:uri "http://josd.github.io/var#x_0"]; r:boundTo [ n3:uri "http://example.org/socrates#Mortal"]];
  r:rule <#lemma4>.

<#lemma3> a r:Extraction;
  r:gives {
    :Socrates a :Human.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/socrates/socrates.n3>].

<#lemma4> a r:Extraction;
  r:gives {
    @forAll var:x_0. {:Socrates a var:x_0} => {:Socrates a var:x_0}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/socrates/socrates-query.n3>].

