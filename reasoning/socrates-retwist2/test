#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/socrates-retwist2/retwist.n3 http://josd.github.io/eye/reasoning/socrates-retwist2/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-retwist2/socrates-query.n3 > socrates-retwist-answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/socrates-retwist2/retwist.n3 http://josd.github.io/eye/reasoning/socrates-retwist2/socrates.n3 --query http://josd.github.io/eye/reasoning/socrates-retwist2/socrates-query.n3 > socrates-retwist-proof.n3
